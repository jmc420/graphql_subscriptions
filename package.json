{
	"name": "GraphQLSubscrriptions",
	"type": "module",
	"version": "1.0.0",
	"dependencies": {
		"@graphql-tools/schema": "^10.0.4",
		"body-parser": "^1.20.2",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-session": "^1.18.0",
		"fast-xml-parser": "^4.4.0",
		"fs-extra": "^11.2.0",
		"graphql": "^16.9.0",
		"graphql-geojson-scalar-types": "^0.1.0",
		"graphql-http": "^1.22.1",
		"graphql-scalars": "^1.23.0",
		"graphql-subscriptions": "^2.0.0",
		"graphql-ws": "^5.16.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.isequal": "^4.5.0",
		"loglevel": "^1.6.7",
		"reflect-metadata": "^0.2.0",
		"wkx": "^0.5.0",
		"ws": "^8.14.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.18.0",
		"@types/jest": "^29.5.10",
		"@types/node": "^20.5.7",
		"jest": "^29.7.0",
		"node-fetch-cookies": "^2.1.1",
		"nodemon": "^3.1.0",
		"typescript": "^5.1.6"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"jest": {
		"collectCoverage": true,
		"moduleFileExtensions": [
			"js"
		],
		"setupFiles": [
			"dotenv/config"
		],
		"testEnvironment": "jest-environment-node"
	},
	"scripts": {
		"all": "npm run compile && npm run server",
		"clean": "rm -fr ./node_modules ./build/*",
		"client": "node build/client/Client.js $*",
		"compile": "tsc $*",
		"debug-server": "node --inspect-brk build/server/main.js $*",
		"debug-test": "NODE_OPTIONS='--experimental-vm-modules' node --inspect-brk=7010 ./node_modules/.bin/jest --runInBand $*",
		"server": "node build/server/main.js $*",
		"server-nodemon": "nodemon build/server/main.js $*",
		"test": "NODE_OPTIONS='--experimental-vm-modules' jest $*"
	}
}